Cell[BoxData[
 RowBox[{
  RowBox[{"StateVector2Map", "[", "r_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "rmod", ",", "l", ",", "m", ",", "n", ",", "\[Delta]", ",", "RA"}], 
     "}"}], ",", "\n", 
    RowBox[{
     RowBox[{"rmod", "=", 
      RowBox[{"Norm", "[", "r", "]"}]}], ";", "\n", 
     RowBox[{"l", "=", 
      RowBox[{
       RowBox[{"r", "[", 
        RowBox[{"[", "1", "]"}], "]"}], "/", "rmod"}]}], ";", "\n", 
     RowBox[{"m", "=", 
      RowBox[{
       RowBox[{"r", "[", 
        RowBox[{"[", "2", "]"}], "]"}], "/", "rmod"}]}], ";", "\n", 
     RowBox[{"n", "=", 
      RowBox[{
       RowBox[{"r", "[", 
        RowBox[{"[", "3", "]"}], "]"}], "/", "rmod"}]}], ";", "\n", 
     RowBox[{"(*", " ", 
      RowBox[{"calculate", " ", "Declination"}], "  ", "*)"}], "\n", 
     RowBox[{"\[Delta]", "=", 
      RowBox[{
       RowBox[{"ArcSin", "[", "n", "]"}], "//", "N"}]}], ";", "\n", 
     RowBox[{"(*", " ", 
      RowBox[{"calulate", " ", "Right", " ", "Asecntion"}], "  ", "*)"}], 
     "\n", 
     RowBox[{"RA", "=", 
      RowBox[{
       RowBox[{"Piecewise", "[", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"ArcCos", "[", 
             FractionBox["l", 
              RowBox[{"Cos", "[", "\[Delta]", "]"}]], "]"}], ",", 
            RowBox[{"m", ">", "0"}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{"2", "\[Pi]"}], "-", 
             RowBox[{"ArcCos", "[", 
              FractionBox["l", 
               RowBox[{"Cos", "[", "\[Delta]", "]"}]], "]"}]}], ",", 
            RowBox[{"m", "\[LessEqual]", "0"}]}], "}"}]}], "}"}], "]"}], "//",
        "N"}]}], ";", "\n", 
     RowBox[{"Return", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"RA", "*", 
         RowBox[{"180", "/", "\[Pi]"}]}], ",", 
        RowBox[{"\[Delta]", "*", 
         RowBox[{"180", "/", "\[Pi]"}]}]}], "}"}], "]"}], ";"}]}], 
   "]"}]}]], "Code",
