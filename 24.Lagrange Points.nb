Cell[BoxData[
 RowBox[{
  RowBox[{"BisectionMethod", "[", 
   RowBox[{"f_", ",", "xl_", ",", "xu_", ",", "E_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"n", ",", "i", ",", "xm", ",", "Xl", ",", "Xu"}], "}"}], ",", 
    "\n", 
    RowBox[{
     RowBox[{"Xl", "=", "xl"}], ";", 
     RowBox[{"Xu", "=", "xu"}], ";", "\n", 
     RowBox[{"n", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Floor", "[", 
         RowBox[{
          FractionBox["1", 
           RowBox[{"Log", "[", "2", "]"}]], "*", 
          RowBox[{"Log", "[", 
           FractionBox[
            RowBox[{"Abs", "[", 
             RowBox[{"xu", "-", "xl"}], "]"}], "E"], "]"}]}], "]"}], "+", 
        "1"}], "//", "N"}]}], ";", "\n", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", "n"}], ",", 
       RowBox[{"i", "++"}], ",", "\n", "    ", 
       RowBox[{
        RowBox[{"xm", "=", 
