Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"m1", "=", 
    RowBox[{"5.974", "*", 
     RowBox[{"10", "^", "24"}]}]}], ";"}], " ", 
  RowBox[{"(*", "  ", "Earth", "  ", "*)"}]}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"m2", "=", 
    RowBox[{"7.348", "*", 
     RowBox[{"10", "^", "22"}]}]}], ";"}], "  ", 
  RowBox[{"(*", "  ", "Moon", "  ", "*)"}]}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"r12", "=", 
    RowBox[{"3.844", "*", 
     RowBox[{"10", "^", "5"}]}]}], ";"}], "  ", 
  RowBox[{"(*", " ", 
   RowBox[{
   "distance", " ", "between", " ", "moon", " ", "and", " ", "earth"}], " ", 
   "*)"}]}], "\n", 
 RowBox[{
  RowBox[{"\[Pi]2", "=", 
   FractionBox["m2", 
    RowBox[{"m1", "+", "m2"}]]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"LagrangePoints", "[", 
   RowBox[{"pi_", ",", "r_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"L4", ",", "L5", ",", "L3", ",", "L2", ",", "L1"}], "}"}], ",", 
    "\n", 
    RowBox[{"(*", " ", 
     RowBox[{
     "we", " ", "know", " ", "that", " ", "lagrange", " ", "points", " ", 
      "are", " ", "5"}], " ", "*)"}], "\n", 
    RowBox[{"(*", " ", 
     RowBox[{
      RowBox[{"get", " ", "L4"}], ",", "L5"}], " ", "*)"}], "\n", 
    RowBox[{
     RowBox[{"L4", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         FractionBox["r", "2"], "-", 
         RowBox[{"pi", "*", "r"}]}], ",", 
        RowBox[{
         FractionBox[
          SqrtBox["3"], "2"], "*", "r"}]}], "}"}]}], ";", "\n", 
     RowBox[{"L5", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         FractionBox["r", "2"], "-", 
         RowBox[{"pi", "*", "r"}]}], ",", 
        RowBox[{
         RowBox[{"-", 
          FractionBox[
           SqrtBox["3"], "2"]}], "*", "r"}]}], "}"}]}], ";", "\n", 
     RowBox[{"(*", " ", 
      RowBox[{
       RowBox[{"we", " ", "will", " ", "get", " ", "L1"}], ",", "L2", ",", 
       RowBox[{
       "L3", " ", "using", " ", "bisection", " ", "method", " ", "to", " ", 
        "solve", " ", "no", " ", "linear", " ", "equation"}]}], "  ", "*)"}], 
     "\n", 
     RowBox[{"(*", " ", 
      RowBox[{"get", " ", "L3"}], "  ", "*)"}], "\n", 
     RowBox[{"L3", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"r", "*", 
         RowBox[{"BisectionMethod", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{
               FractionBox[
                RowBox[{"1", "-", "pi"}], 
                SuperscriptBox[
                 RowBox[{"Abs", "[", 
                  RowBox[{"#", "+", "pi"}], "]"}], "3"]], "*", 
               RowBox[{"(", 
                RowBox[{"#", "+", "pi"}], ")"}]}], "+", 
              RowBox[{
               FractionBox["pi", 
                SuperscriptBox[
                 RowBox[{"Abs", "[", 
                  RowBox[{"#", "+", "pi", "-", "1"}], "]"}], "3"]], "*", 
               RowBox[{"(", 
                RowBox[{"#", "+", "pi", "-", "1"}], ")"}]}], "-", "#"}], 
             ")"}], "&"}], ",", 
           RowBox[{"-", "1.5"}], ",", 
           RowBox[{"-", "1"}], ",", 
           RowBox[{"10", "^", 
            RowBox[{"-", "8"}]}]}], "]"}]}], ",", "0"}], "}"}]}], ";", "\n", 
     RowBox[{"(*", " ", 
      RowBox[{"get", " ", "L2"}], "  ", "*)"}], "\n", 
     RowBox[{"L2", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"r", "*", 
         RowBox[{"BisectionMethod", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{
               FractionBox[
                RowBox[{"1", "-", "pi"}], 
                SuperscriptBox[
                 RowBox[{"Abs", "[", 
                  RowBox[{"#", "+", "pi"}], "]"}], "3"]], "*", 
               RowBox[{"(", 
                RowBox[{"#", "+", "pi"}], ")"}]}], "+", 
              RowBox[{
               FractionBox["pi", 
                SuperscriptBox[
                 RowBox[{"Abs", "[", 
                  RowBox[{"#", "+", "pi", "-", "1"}], "]"}], "3"]], "*", 
               RowBox[{"(", 
                RowBox[{"#", "+", "pi", "-", "1"}], ")"}]}], "-", "#"}], 
             ")"}], "&"}], ",", "1", ",", "1.5", ",", 
           RowBox[{"10", "^", 
            RowBox[{"-", "8"}]}]}], "]"}]}], ",", "0"}], "}"}]}], ";", "\n", 
     RowBox[{"(*", " ", 
      RowBox[{"get", " ", "L1"}], "  ", "*)"}], "\n", 
     RowBox[{"L1", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"r", "*", 
         RowBox[{"BisectionMethod", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{
               FractionBox[
                RowBox[{"1", "-", "pi"}], 
                SuperscriptBox[
                 RowBox[{"Abs", "[", 
                  RowBox[{"#", "+", "pi"}], "]"}], "3"]], "*", 
               RowBox[{"(", 
                RowBox[{"#", "+", "pi"}], ")"}]}], "+", 
              RowBox[{
               FractionBox["pi", 
                SuperscriptBox[
                 RowBox[{"Abs", "[", 
                  RowBox[{"#", "+", "pi", "-", "1"}], "]"}], "3"]], "*", 
               RowBox[{"(", 
                RowBox[{"#", "+", "pi", "-", "1"}], ")"}]}], "-", "#"}], 
             ")"}], "&"}], ",", 
           RowBox[{"0.75", "-", 
            RowBox[{"2", "pi"}]}], ",", 
           RowBox[{"1.25", "-", 
            RowBox[{"2", "pi"}]}], ",", 
           RowBox[{"10", "^", 
            RowBox[{"-", "8"}]}]}], "]"}]}], ",", "0"}], "}"}]}], ";", "\n", 
     RowBox[{"Return", "[", 
      RowBox[{"{", 
       RowBox[{"L1", ",", "L2", ",", "L3", ",", "L4", ",", "L5"}], "}"}], 
      "]"}], ";"}]}], "]"}]}]}], "Code",
